class LineDrawer {
  method draw() {
    call sdl_init(640,480);
    call sdl_clear();
    call sdl_line(100, 100, 400, 300);
    call sdl_line(100, 200, 400, 400);
    call sdl_present();
  }
  method erase() {
    call sdl_erase_line(100, 100, 400, 300);
    call sdl_present();
  }
}

var dl1 = new LineDrawer();
send dl1.draw();
