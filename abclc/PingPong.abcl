class Pinger {
  method init() {
    print("Pinger starting");
    send ponger.ping();
  }
  method pong() {
    print("Pinger got pong");
    send ponger.ping();
  }
}

class Ponger {
  method ping() {
    print("Ponger got ping");
    send pinger.pong();
  }
}

var pinger = new Pinger();
var ponger = new Ponger();
