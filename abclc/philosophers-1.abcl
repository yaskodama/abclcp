object Philosopher0 {
  float count = 0.0;
  method think_eat() {
    send Fork0 take;
    send Fork1 take;
    count = count + 1.0;
    call print(count);
    send Fork0 put;
    send Fork1 put;
    send self think_eat;
  }
};

