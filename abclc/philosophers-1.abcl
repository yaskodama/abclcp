object Fork0 { float state = 0.0; method take() { if (state == 0.0) { state = 1.0; } else { send self take; } } method put() { state = 0.0; } };
object Fork1 { float state = 0.0; method take() { if (state == 0.0) { state = 1.0; } else { send self take; } } method put() { state = 0.0; } };
